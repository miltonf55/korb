<script>
    function date(){
        var fecha = new Date(); 
        var mes = fecha.getMonth()+1;
        var dia = fecha.getDate();
        var ano = fecha.getFullYear();
        if(dia<10)
            dia='0'+dia; 
        if(mes<10)
            mes='0'+mes;
        document.getElementById('date').value=ano+"-"+mes+"-"+dia;
    }
    setTimeout(date,1500);
</script>
